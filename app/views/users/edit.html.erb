<h1>Edit user</h1>

<%= form_for(@user) do |f| %>
  <%= render 'shared/error_messages', :object => f.object %>
  <% if @user.isAdmin? %>
    <%= render 'fields', :f => f %>
  <% else %>
    <%= render 'fields1', :f => f %>
  <% end %>
  <div class="actions">
    <%= f.submit "Update" %>
  </div>
     
  
<% end %>
